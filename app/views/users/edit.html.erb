<% provide :title, t(:setting).pluralize %> <a href="http://gravatar.com/emails" target="_blank"><%= gravatar_for @user, size: 300 %></a><%= form_for @user, html: { class: "pull-left" } do |f| %>  <%= render "shared/error_messages", object: f.object %>  <%= render "fields", f: f %>  <%= f.submit t(:update_verb), class: "btn btn-primary" %><% end %>