<%= form_for post, remote: true do |f| %>  <%= render "shared/error_messages", object: f.object %>    <div class="tag_list">    <%= f.text_field :title, class: "span6", placeholder: t(:title) %>    <%= f.text_field :tag_list, class: "span6 tag_list_textbox", placeholder: t(:tag_list_placeholder), autocomplete: "off", onkeyup: "events.tagListTextBoxKeyUp(event, '#{tags_path}');" %>  </div>  <%= f.text_area :content, rows: 3, placeholder: t(:new_post_message) %>    <%= submit_tag t(:post_verb), class: "btn btn-primary", data: { disable_with: t(:post_verb) } %>    <% if local_assigns[:show_cancel] %>    <%= submit_tag t(:cancel), class: "btn btn-primary", type: "button", onclick: "$('#post_" + post.id.to_s + "').replaceWith('" + escape_javascript(render "posts/post", post: post) + "');" %>  <% else %>    <%= submit_tag t(:search_verb), class: "btn btn-primary", data: { disable_with: t(:search_verb) } %>  <% end %><% end %>